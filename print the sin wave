void setup() {
  Serial.begin(9600);
}

void loop() {
  for (int i = 0; i < 360; i += 5) {
    float y1 = 1 + sin(radians(i));
    float y2 = 1 + sin(radians(i + 90));
    float y3 = 1 + sin(radians(i + 180));

    Serial.print(y1);
    Serial.print(" ");
    Serial.print(y2);
    Serial.print(" ");
    Serial.println(y3);   // newline at end

    delay(1000);
  }
}
