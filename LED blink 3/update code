int led1=6;
int led2=7;
int led3=8;

void setup(){
  pinMode(led1,OUTPUT);
  pinMode(led2,OUTPUT);
  pinMode(led3,OUTPUT);
}
void loop(){
  ledblink(led1);
  ledblink(led2);
  ledblink(led3);
}
void ledblink(int led){
  digitalWrite(led,HIGH);
  delay(100);
  digitalWrite(led,LOW);
  delay(100);
  
}
